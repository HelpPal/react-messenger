{
  "name": "ProjectX",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean --force && npm i",
    "test:watch": "jest --watch",
    "updateSnapshot": "jest --updateSnapshot",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:hockeyapp": "cd android && ./gradlew assembleRelease && puck -submit=auto app/build/outputs/apk/app-release.apk",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    "storybook": "storybook start -p 7007",
    "lint": "standard --verbose | snazzy",
    "lintdiff": "git diff --name-only --cached --relative | grep '\\.js$' | xargs standard | snazzy",
    "fixcode": "standard --fix",
    "git-hook": "npm run lint -s && npm run test -s",
    "appconfig:clean": "rm -f ./appconfig.js",
    "appconfig:local": "npm run appconfig:clean && sed -- \"s/localhost/`my-local-ip`/\" ./appconfig.js.example > appconfig.js",
    "appconfig:dev": "npm run appconfig:clean && cp ./appconfig.js.dev appconfig.js",
    "appconfig:prod": "npm run appconfig:clean && cp ./appconfig.js.prod appconfig.js"
  },
  "dependencies": {
    "@ardentlabs/react-native-emoji": "^1.2.1",
    "@mapbox/react-native-mapbox-gl": "^6.1.0",
    "events": "2.0.0",
    "lodash": "4.17.5",
    "moment": "2.21.0",
    "moment-timezone": "0.5.14",
    "prop-types": "15.6.1",
    "querystring": "0.2.0",
    "querystringify": "1.0.0",
    "react": "16.0.0-alpha.12",
    "react-native": "0.53.3",
    "react-native-animatable": "1.2.4",
    "react-native-communications": "2.2.1",
    "react-native-device-info": "0.15.3",
    "react-native-elements": "^0.19.0",
    "react-native-fabric": "0.5.1",
    "react-native-fabric-crashlytics": "0.1.8",
    "react-native-fast-image": "4.0.0",
    "react-native-fbsdk": "git+https://github.com/bdavid68/react-native-fbsdk.git#master",
    "react-native-geolocation-service": "1.0.3",
    "react-native-image-progress": "git+https://github.com/bdavid68/react-native-image-progress.git#openmed",
    "react-native-keyboard-aware-scroll-view": "0.3.0",
    "react-native-maps": "0.20.1",
    "react-native-navigation": "git+https://github.com/OpenMed/react-native-navigation.git#openmed",
    "react-native-open-maps": "0.1.3",
    "react-native-permissions": "1.0.1",
    "react-native-progress": "3.4.0",
    "react-native-swiper": "1.5.13",
    "react-native-vector-icons": "4.5.0",
    "react-redux": "5.0.2",
    "recompose": "0.26.0",
    "redux": "3.6.0",
    "redux-actions": "2.0.3",
    "redux-logger": "2.7.4",
    "redux-persist": "4.0.1",
    "redux-persist-migrate": "4.0.1",
    "redux-persist-transform-filter": "0.0.6",
    "redux-saga": "0.15.3",
    "redux-thunk": "2.1.0",
    "reselect": "3.0.1",
    "rmc-picker": "5.0.3",
    "seamless-immutable": "7.1.2",
    "url-parse": "1.1.9"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-jest": "22.4.3",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-jest": "^23.0.0-alpha.0",
    "babel-preset-react-native": "4.0.0",
    "babel-preset-react-native-stage-0": "1.0.1",
    "enzyme": "2.8.0",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-react-native": "^3.2.1",
    "flow-bin": "0.53.1",
    "flow-typed": "2.1.5",
    "husky": "0.13.3",
    "jest": "22.4.3",
    "jest-enzyme": "3.5.0",
    "jest-repl": "19.0.2",
    "my-local-ip": "1.0.0",
    "react-addons-test-utils": "15.4.0",
    "react-apollo": "1.4.2",
    "react-devtools": "2.1.3",
    "react-native-mock": "0.3.1",
    "react-test-renderer": "16.0.0-alpha.12",
    "remote-redux-devtools": "0.5.10",
    "remotedev-server": "0.2.2",
    "sc-broker-cluster": "4.0.0"
  },
  "jest": {
    "preset": "react-native",
    "bail": true,
    "verbose": true,
    "automock": false,
    "cacheDirectory": "<rootDir>/.jest-cache",
    "moduleNameMapper": {
      "^React$": "<rootDir>/node_modules/react"
    },
    "setupFiles": [
      "<rootDir>/jest-setup.js"
    ],
    "haste": {
      "defaultPlatform": "ios",
      "platforms": [
        "android",
        "ios",
        "native",
        "windows"
      ],
      "providesModuleNodeModules": [
        "react-native"
      ]
    }
  },
  "rnpm": {
    "assets": [
      "src/fonts/assets"
    ]
  }
}
